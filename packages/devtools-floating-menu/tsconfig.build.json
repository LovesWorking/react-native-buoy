{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "declaration": true,
    "declarationOnly": true,
    "emitDeclarationOnly": true,
    "noEmit": false
  },
  "exclude": [
    "node_modules",
    "lib",
    "../env-tools/**/*",
    "../network/**/*",
    "../storage/**/*",
    "../react-query/**/*",
    "../route-events/**/*",
    "**/*.test.ts",
    "**/*.test.tsx"
  ]
}

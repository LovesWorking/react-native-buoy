{
  "name": "@react-buoy/storage",
  "version": "0.1.8",
  "description": "storage package",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./storage": {
      "source": "./src/storage/index.ts",
      "types": "./lib/typescript/src/storage/index.d.ts",
      "default": "./lib/module/storage/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "storage": [
        "lib/typescript/src/storage/index.d.ts"
      ],
      "storage/*": [
        "lib/typescript/src/storage/*"
      ]
    }
  },
  "files": [
    "src",
    "lib"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "bob build",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf lib",
    "test": "pnpm run typecheck",
    "postinstall": "echo \"Run pnpm build to compile this package\""
  },
  "dependencies": {
    "@react-buoy/shared-ui": "workspace:*",
    "@tanstack/react-query": "^5.89.0"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": ">=1.0.0",
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@types/react": "^19.1.0",
    "@types/react-native": "^0.73.0",
    "typescript": "~5.8.3"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LovesWorking/react-native-buoy.git",
    "directory": "packages/storage"
  },
  "bugs": {
    "url": "https://github.com/LovesWorking/react-native-buoy/issues"
  },
  "homepage": "https://github.com/LovesWorking/react-native-buoy/tree/main/packages/storage#readme",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  }
}
